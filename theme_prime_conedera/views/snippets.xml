<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <template id="snippets" inherit_id="website.snippets">
        <t t-snippet="website.s_banner" position="before">
            <t t-snippet="theme_prime_conedera.s_d_2_column_snippet_conedera" string="2 Col Deal Conedera" group="theme_prime_dynamic">
                <keywords>products, dynamic</keywords>
            </t>
        </t>
        <t t-snippet="website.s_banner_15" position="before">
            <t t-snippet="theme_prime_conedera.s_banner_16" string="Banner Conedera" group="theme_prime_banner">
                <keywords>products, dynamic</keywords>
            </t>
        </t>
    </template>

    <template id="snippet_options_inherit" inherit_id="website.snippet_options">
        <xpath expr="//we-select[@data-variable='footer-template']/we-button[last()]" position="after">
            <we-button title="Prime Footer Style - 11" data-name="tp_footer_style_11_opt" data-customize-website-views="theme_prime_conedera.template_footer_style_11" data-customize-website-variable="'prime_style_11'" data-img="/theme_prime/static/src/img/options/footer_1.svg"/>
        </xpath>
    </template>

</odoo>
