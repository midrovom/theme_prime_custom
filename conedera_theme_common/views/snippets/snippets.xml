<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <template id="add_my_snippet_to_columns" inherit_id="web_editor.snippets">
        <xpath expr="//snippets[@id='snippet_structure']" position="inside">
            <t t-snippet="conedera_theme_common.s_key_images_custom"
               string="Key Images Custom"
               group="columns">
                <keywords>columns, gallery, pictures, photos, media, showcase, portfolio, custom</keywords>
            </t>
        </xpath>
    </template>
</odoo>
